create or replace TABLE FEATURES.RUNNING.ACTIVITIES_WITH_WEATHER (
	ACTIVITY_ID NUMBER(38,0),
	NAME VARCHAR(16777216),
	START_DATE DATE,
	START_YEAR NUMBER(4,0),
	START_MONTH NUMBER(2,0),
	START_DATE_UTC TIMESTAMP_NTZ(9),
	START_DATE_HOUR_UTC NUMBER(2,0),
	START_LATITUDE FLOAT,
	START_LONGITUDE FLOAT,
	POSTAL_CODE VARCHAR(16777216),
	TEMP_IN_FARENHEIT NUMBER(38,1),
	TEMP_FEEL_IN_FARENHEIT NUMBER(38,1),
	CLOUD_COVER_PCT NUMBER(38,0),
	TEST_COL2 VARCHAR(16777216)
);
